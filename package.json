{
  "name": "fetch-actions",
  "version": "0.1.0",
  "description": "Manage fetch with flux-standard-actions dispatched from a saga or thunk.",
  "main": "lib/index.js",
  "module": "es/index.js",
  "jsnext:main": "es/index.js",
  "files": [
    "dist",
    "lib",
    "es",
    "src"
  ],
  "author": "Grady Kuhnline <github@heygrady.net> (https://github.com/heygrady)",
  "repository": {
    "type": "git",
    "url": "https://github.com/heygrady/fetch-actions.git"
  },
  "keywords": [
    "redux",
    "fetch",
    "flux-standard-actions"
  ],
  "bugs": {
    "url": "https://github.com/heygrady/fetch-actions/issues"
  },
  "homepage": "https://github.com/heygrady/fetch-actions",
  "license": "MIT",
  "config": {
    "account": "heygrady",
    "name": "fetch-actions"
  },
  "scripts": {
    "clean": "rimraf lib dist es coverage",
    "dev": "yarn build:commonjs --watch",
    "build:commonjs": "cross-env MODULES_ENV=commonjs babel src --out-dir lib",
    "build:esmodules": "cross-env MODULES_ENV=esmodules babel src --out-dir es",
    "build:umd": "rollup -c",
    "build": "yarn clean && yarn run build:commonjs && yarn run build:esmodules && yarn run build:umd",
    "format": "prettier --write \"{example,src,test}/**/*.{js,jsx,ts,tsx}\" .babelrc.js rollup.config.js .eslintrc.json README.md \"docs/**/*.md\" && yarn lint --fix || true",
    "lint": "eslint --ext .js,.mjs,.jsx,.ts,.tsx src test",
    "dedupe": "npx yarn-deduplicate yarn.lock && yarn install",
    "pretest": "yarn lint",
    "prepare": "yarn test && yarn clean && yarn build",
    "preversion": "yarn format",
    "postversion": "npm publish --access public",
    "postpublish": "git push origin --all && git push origin --tags",
    "test": "jest",
    "coverage": "yarn test --coverage && npx http-server ./coverage/lcov-report -p4001 -c-1"
  },
  "dependencies": {
    "tiny-invariant": "^1.0.6",
    "tiny-warning": "^1.0.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@zumper/babel-preset-react-app": "^9.0.1-zumper.16",
    "babel-eslint": "^10.0.2",
    "babel-jest": "^24.9.0",
    "babel-plugin-module-resolver": "^3.2.0",
    "cross-env": "^5.2.0",
    "cross-fetch": "^3.0.4",
    "eslint": "^6.2.1",
    "eslint-config-prettier": "^6.1.0",
    "eslint-config-react-app": "^5.0.1",
    "eslint-config-standard": "^14.0.1",
    "eslint-import-resolver-babel-module": "^5.1.0",
    "eslint-plugin-flowtype": "^3.13.0",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jest": "^22.15.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-node": "^9.1.0",
    "eslint-plugin-prettier": "^3.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-react": "^7.14.3",
    "eslint-plugin-react-hooks": "^2.0.1",
    "eslint-plugin-standard": "^4.0.1",
    "fs-extra": "^8.1.0",
    "jest": "^24.9.0",
    "lodash": "^4.17.15",
    "prettier": "^1.18.2",
    "rimraf": "^3.0.0",
    "rollup": "^1.20.1",
    "rollup-plugin-babel": "^4.3.3",
    "rollup-plugin-commonjs": "^10.0.2",
    "rollup-plugin-node-resolve": "^5.2.0",
    "rollup-plugin-replace": "^2.2.0",
    "rollup-plugin-terser": "^5.1.1"
  },
  "npmName": "fetch-actions",
  "npmFileMap": [
    {
      "basePath": "/dist/",
      "files": [
        "*.js"
      ]
    }
  ],
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!src/**/*.d.ts"
    ],
    "coverageDirectory": "<rootDir>/coverage/",
    "coveragePathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/es",
      "<rootDir>/lib",
      "<rootDir>/node_modules"
    ],
    "resolver": "jest-pnp-resolver",
    "testMatch": [
      "<rootDir>/test/**/*.spec.js"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/dist",
      "<rootDir>/es",
      "<rootDir>/lib",
      "<rootDir>/node_modules"
    ],
    "testURL": "https://localhost"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "browserslist": [
    ">0.2%",
    "Chrome 41",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
